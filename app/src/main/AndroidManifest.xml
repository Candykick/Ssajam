<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hua15.candykick.ssajam" >

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" >
    </uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ClauseActivity"
            android:label="@string/title_activity_clause" >
        </activity>
        <activity
            android:name=".JoinActivity"
            android:label="@string/title_activity_join" >
        </activity>
        <activity
            android:name=".MapActivity"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name=".RoomInfoActivity"
            android:label="@string/title_activity_room_info" >
        </activity>
        <activity
            android:name=".AdressRoomActivity"
            android:label="@string/title_activity_adress_room" >
        </activity>
        <activity
            android:name=".SellerMainActivity"
            android:label="@string/title_activity_seller_main" >
        </activity>
    </application>

</manifest>
